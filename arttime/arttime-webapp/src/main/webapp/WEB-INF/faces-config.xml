<?xml version="1.0"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>logout</from-outcome>
            <to-view-id>/pages/timesheet/timesheet.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>settings</from-outcome>
            <to-view-id>/pages/settings/security.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>manageEfforts</from-outcome>
            <to-view-id>/pages/manageEfforts/manageEfforts.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>accessToDepartments</from-outcome>
            <to-view-id>/pages/access/accessToDepartments.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>timesheet</from-outcome>
            <to-view-id>/pages/timesheet/timesheet.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>hourTypes</from-outcome>
            <to-view-id>/pages/hourType/hourTypes.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>projects</from-outcome>
            <to-view-id>/pages/project/projects.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>employees</from-outcome>
            <to-view-id>/pages/employee/employees.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>calendars</from-outcome>
            <to-view-id>/pages/calendar/calendars.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>actualTimeProblemsNotification</from-outcome>
            <to-view-id>/pages/notifications/actualTimeProblemsNotification.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>reports</from-outcome>
            <to-view-id>/pages/reports/reports.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>security</from-outcome>
            <to-view-id>/pages/settings/security.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>externalDataProviders</from-outcome>
            <to-view-id>/pages/settings/externalDataProviders.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>smtp</from-outcome>
            <to-view-id>/pages/settings/smtp.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>synchronization</from-outcome>
            <to-view-id>/pages/settings/synchronization.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>notification</from-outcome>
            <to-view-id>/pages/settings/notification.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>links</from-outcome>
            <to-view-id>/pages/settings/links.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pages/project/projects.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>edit</from-outcome>
            <to-view-id>/pages/project/editProject.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>addNew</from-outcome>
            <to-view-id>/pages/project/createProject.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>search</from-outcome>
            <to-view-id>/pages/project/projects.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pages/project/editProject.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/pages/project/projects.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>update</from-outcome>
            <to-view-id>/pages/project/editProject.xhtml</to-view-id>
            <redirect include-view-params="true">
                <redirect-param>
                    <name>project</name>
                    <value>#{projectBean.project.id}</value>
                </redirect-param>
            </redirect>
        </navigation-case>
        <navigation-case>
            <from-outcome>addNew</from-outcome>
            <to-view-id>/pages/project/createSubproject.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>edit</from-outcome>
            <to-view-id>/pages/project/editProject.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pages/project/createProject.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/pages/project/projects.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>create</from-outcome>
            <to-view-id>/pages/project/projects.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pages/hourType/hourTypes.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>edit</from-outcome>
            <to-view-id>/pages/hourType/editHourType.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>addNew</from-outcome>
            <to-view-id>/pages/hourType/createHourType.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pages/hourType/editHourType.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/pages/hourType/hourTypes.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>update</from-outcome>
            <to-view-id>/pages/hourType/hourTypes.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pages/hourType/createHourType.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/pages/hourType/hourTypes.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>create</from-outcome>
            <to-view-id>/pages/hourType/hourTypes.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pages/employee/employees.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>edit</from-outcome>
            <to-view-id>/pages/employee/editEmployee.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>search</from-outcome>
            <to-view-id>/pages/employee/employees.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pages/employee/editEmployee.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>update</from-outcome>
            <to-view-id>/pages/employee/employees.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/pages/employee/employees.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pages/calendar/calendars.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>edit</from-outcome>
            <to-view-id>/pages/calendar/editCalendar.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>addNew</from-outcome>
            <to-view-id>/pages/calendar/createCalendar.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>setDepartmentCalendars</from-outcome>
            <to-view-id>/pages/calendar/calendarsByDepartments.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pages/calendar/createCalendar.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>create</from-outcome>
            <to-view-id>/pages/calendar/calendarsByDepartments.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/pages/calendar/calendars.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pages/calendar/calendarsByDepartments.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/pages/calendar/calendars.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>submit</from-outcome>
            <to-view-id>/pages/calendar/calendars.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pages/calendar/editCalendar.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>update</from-outcome>
            <to-view-id>/pages/calendar/calendars.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/pages/calendar/calendars.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pages/project/createSubproject.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>create</from-outcome>
            <to-view-id>/pages/project/editProject.xhtml</to-view-id>
            <redirect include-view-params="true">
                <redirect-param>
                    <name>project</name>
                    <value>#{projectBean.project.getMaster().id}</value>
                </redirect-param>
                <redirect-param>
                    <name>tab</name>
                    <value>1</value>
                </redirect-param>
            </redirect>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/pages/project/editProject.xhtml</to-view-id>
            <redirect include-view-params="true">
                <redirect-param>
                    <name>project</name>
                    <value>#{projectBean.project.getMaster().id}</value>
                </redirect-param>
                <redirect-param>
                    <name>tab</name>
                    <value>1</value>
                </redirect-param>
            </redirect>
        </navigation-case>
    </navigation-rule>
    <render-kit>
        <renderer>
            <component-family>org.primefaces.component</component-family>
            <renderer-type>org.primefaces.component.OutputLabelRenderer</renderer-type>
            <renderer-class>com.artezio.arttime.web.components.CustomOutputLabelRenderer</renderer-class>
        </renderer>
    </render-kit>
    <application>
        <locale-config>
            <default-locale>ru</default-locale>
            <supported-locale>ru</supported-locale>
            <supported-locale>en</supported-locale>
        </locale-config>
        <resource-bundle>
            <base-name>ValidationMessages</base-name>
            <var>ValidationMessages</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>build-info</base-name>
            <var>buildInfo</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>com.artezio.arttime.i18n.messages</base-name>
            <var>messages</var>
        </resource-bundle>
        <message-bundle>com.artezio.arttime.i18n.messages</message-bundle>
    </application>
    <factory>
        <exception-handler-factory>com.artezio.arttime.web.exception_handler.ExceptionHandlerFactory
        </exception-handler-factory>
    </factory>
</faces-config>
